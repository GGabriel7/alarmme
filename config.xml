<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.myProject.AlarmMe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AlarmMe</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <icon src="resources/icon.png" density="xxxhdpi" />
    <icon src="resources/icon.png" density="xxhdpi" />
    <icon src="resources/icon.png" density="xhdpi" />
    <icon src="resources/icon.png" density="hdpi" />
    <icon src="resources/icon.png" density="mdpi" />

    <icon src="resources/icon.png" width="60" height="60" />
    <icon src="resources/icon.png" width="120" height="120" />
    <icon src="resources/icon.png" width="76" height="76" />
    <icon src="resources/icon.png" width="152" height="152" />
</widget>
